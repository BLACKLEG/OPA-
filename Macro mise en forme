Sub Process()
    Dim ws As Worksheet, v
    For Each ws In ThisWorkbook.Worksheets
        
        
        ws.Columns(1).Delete Shift:=xlUp
        ws.Rows("1:1").Delete Shift:=xlUp


        ws.Columns("A:S").ColumnWidth = 19.86
        

        ws.Range("A2:S10").HorizontalAlignment = xlCenter
        ws.Range("A2:S10").VerticalAlignment = xlCenter
        ws.Range("A2:S10").WrapText = True
  
  
        ws.Range("A2:S10").Borders(xlEdgeTop).LineStyle = xlContinuous
        ws.Range("A2:S10").Borders(xlEdgeLeft).LineStyle = xlContinuous
        ws.Range("A2:S10").Borders(xlEdgeBottom).LineStyle = xlContinuous
        ws.Range("A2:S10").Borders(xlEdgeRight).LineStyle = xlContinuous
        ws.Range("A2:S10").Borders(xlInsideVertical).LineStyle = xlContinuous
        ws.Range("A2:S10").Borders(xlInsideHorizontal).LineStyle = xlContinuous


    Next ws
    
End Sub

